{
  "name": "@wrio/bizcom-sdk",
  "version": "0.1.1",
  "description": "BizCom Embed SDK - Web components for business processes",
  "type": "module",
  "main": "./dist/bizcom.js",
  "module": "./dist/bizcom.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "tsc && vite build -c vite.config.demo.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bizcom",
    "bpm",
    "workflow",
    "web-components",
    "embed"
  ],
  "author": "WRIO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/webRunes/bizcom-sdk.git"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "dependencies": {}
}
